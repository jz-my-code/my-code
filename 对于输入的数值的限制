//this is a project which improves my comperhend for the range.
#include <stdio.h>
int main()
{
	int a;
	int b;
	scanf("%d%d",&a,&b);
	while(-100>a||a>100||b>100||b<-100)//this type is necessary.this is a operation which limits the range.
	{
		printf("your input is wrong,please give the number again:");
		scanf("%d%d",&a,&b);
	}
	printf("%d",a*b);
	return 0;
}
